  keywords = {acid-base disorders,Acid-Base Imbalance,acid/base and electrolyte disorders,arterial blood gas analysis,arterial blood gas interpretation,arterial blood gas measurement,Bicarbonates,Blood Gas Analysis,Carbon Dioxide,Humans,Hydrogen-Ion Concentration},
