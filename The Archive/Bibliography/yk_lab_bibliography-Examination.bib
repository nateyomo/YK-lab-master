}

@article{nazariPsychometricPropertiesPatientSpecific2022,
  title = {Psychometric {{Properties}} of the {{Patient-Specific Functional Scale}} in {{Patients}} with {{Low Back Pathology}}: {{A Systematic Review}} and {{Meta-Analysis}}},
  shorttitle = {Psychometric {{Properties}} of the {{Patient-Specific Functional Scale}} in {{Patients}} with {{Low Back Pathology}}},
