@article{donofrioGuillainBarreSyndrome2017,
  title = {Guillain-{{Barr{\'e} Syndrome}}},
  author = {Donofrio, Peter D.},
  year = {2017},
  month = oct,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
  volume = {23},
  number = {5},
