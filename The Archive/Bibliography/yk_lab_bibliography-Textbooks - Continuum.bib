@article{glassHypoxicIschemicEncephalopathyOther2018,
  title = {Hypoxic-{{Ischemic Encephalopathy}} and {{Other Neonatal Encephalopathies}}},
  author = {Glass, Hannah C.},
  year = {2018},
  month = feb,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
  volume = {24},
  number = {1},
