  keywords = {Distal radioulnar joint,Instability,Treatment,Triangular fibrocartilage complex},
  file = {/Users/nathanielyomogida/Zotero/storage/FRCC2PPI/Rodrguez-Merchn et al (2022) Distal Radioulnar Joint Instability.pdf}
}

