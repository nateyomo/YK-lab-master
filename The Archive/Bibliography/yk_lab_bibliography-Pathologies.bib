  keywords = {Adult,Exercise Therapy,exercise-based management,Female,Finish reading,glenohumeral,Humans,impairment,Joint Instability,Level IV,Multidirectional instability,Muscle Strength,patient focused,shoulder,Shoulder Dislocation,Shoulder Joint,Systematic Review,Treatment Outcome},
