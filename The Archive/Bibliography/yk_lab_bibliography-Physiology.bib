@article{ariasRelationshipCholineBioavailability2020,
  title = {The {{Relationship}} between {{Choline Bioavailability}} from {{Diet}}, {{Intestinal Microbiota Composition}}, and {{Its Modulation}} of {{Human Diseases}}},
