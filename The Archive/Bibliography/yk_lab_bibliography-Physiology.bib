  author = {Xie, Meng and Gol'din, Pavel and Herdina, Anna Nele and Estefa, Jordi and Medvedeva, Ekaterina V. and Li, Lei and Newton, Phillip T. and Kotova, Svetlana and Shavkuta, Boris and Saxena, Aditya and Shumate, Lauren T. and Metscher, Brian D. and Gro{\ss}schmidt, Karl and Nishimori, Shigeki and Akovantseva, Anastasia and Usanova, Anna P. and Kurenkova, Anastasiia D. and Kumar, Anoop and Arregui, Irene Linares and Tafforeau, Paul and Fried, Kaj and Carlstr{\"o}m, Mattias and Simon, Andr{\'a}s and Gasser, Christian and Kronenberg, Henry M. and Bastepe, Murat and Cooper, Kimberly L. and Timashev, Peter and Sanchez, Sophie and Adameyko, Igor and Eriksson, Anders and Chagin, Andrei S.},
  year = {2020},
  month = oct,
  journal = {eLife},
  volume = {9},
  pages = {e55212},
  issn = {2050-084X},
  doi = {10.7554/eLife.55212},
  abstract = {Growth plate and articular cartilage constitute a single anatomical entity early in development but later separate into two distinct structures by the secondary ossification center (SOC). The reason for such separation remains unknown. We found that evolutionarily SOC appears in animals conquering the land - amniotes. Analysis of the ossification pattern in mammals with specialized extremities (whales, bats, jerboa) revealed that SOC development correlates with the extent of mechanical loads. Mathematical modeling revealed that SOC reduces mechanical stress within the growth plate. Functional experiments revealed the high vulnerability of hypertrophic chondrocytes to mechanical stress and showed that SOC protects these cells from apoptosis caused by extensive loading. Atomic force microscopy showed that hypertrophic chondrocytes are the least mechanically stiff cells within the growth plate. Altogether, these findings suggest that SOC has evolved to protect the hypertrophic chondrocytes from the high mechanical stress encountered in the terrestrial environment.},
  langid = {english},
  pmcid = {PMC7581430},
  pmid = {33063669},
  keywords = {Animals,bat,Biomechanical Phenomena,Cell Differentiation,Cell Proliferation,chondrocytes,Chondrocytes,developmental biology,evolutionary biology,growth plate,Growth Plate,jerboa,Mice,Mice Inbred C57BL,mouse,Osteogenesis,rat,Rats,Rats Sprague-Dawley,secondary ossification,Stress Mechanical,To Read,whale},
  file = {/Users/nathanielyomogida/Zotero/storage/EKI2G2DZ/Xie et al (2020) Secondary ossification center induces and protects growth plate structure.pdf}
}

