@article{shawConstraintinducedMovementTherapy2005,
  title = {Constraint-Induced Movement Therapy for Recovery of Upper-Limb Function Following Traumatic Brain Injury},
  author = {Shaw, Sharon E. and Morris, David M. and Uswatte, Gitendra and McKay, Staci and Meythaler, Jay M. and Taub, Edward},
