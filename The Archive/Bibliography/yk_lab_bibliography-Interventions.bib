@article{parkEffectEarthingMat2022,
  title = {The {{Effect}} of {{Earthing Mat}} on {{Stress-Induced Anxiety-like Behavior}} and {{Neuroendocrine Changes}} in the {{Rat}}},
  author = {Park, Hyun-Jung and Jeong, Woojin and Yu, Hyo Jeong and Ye, Minsook and Hong, Yunki and Kim, Minji and Kim, Ji Youn and Shim, Insop},
