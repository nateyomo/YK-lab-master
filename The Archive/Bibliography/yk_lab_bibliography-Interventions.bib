  pmcid = {PMC7039487},
  pmid = {32148616},
  keywords = {20-mile run,70 VO2 max,C-reactive protein,endurance trained individuals,Inflammation,Intermittent Pneumatic Compression Devices,muscle recovery,pneumatic compression,stretchaffect reading list,To Read},
