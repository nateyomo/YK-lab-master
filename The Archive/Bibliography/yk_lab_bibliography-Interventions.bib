  shorttitle = {Blood {{Flow Restriction Training}} in {{Patients With Rotator Cuff Tendinopathy}}},
  author = {Kara, Dilara and Ozcakar, Levent and Demirci, Serdar and Huri, Gazi and Duzgun, Irem},
  year = {2024},
  month = jan,
  journal = {Clinical Journal of Sport Medicine: Official Journal of the Canadian Academy of Sport Medicine},
  volume = {34},
